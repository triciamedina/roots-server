    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.0",
    "moment": "^2.26.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "path": "^0.12.7",
    "pg": "^8.1.0",
    "plaid": "^4.11.0",
    "winston": "^3.2.1",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
